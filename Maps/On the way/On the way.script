procedure OnTick;
begin
  if States.GameTime = 50 then
    Actions.ShowMsg(0, States.Text(1));
  if States.GameTime = 2200 then
    Actions.ShowMsg(0, States.Text(2));
  if States.GameTime = 4200 then
    Actions.ShowMsg(0, States.Text(3));
  if States.GameTime = 48000 then
    Actions.ShowMsg(0, States.Text(4));
end;