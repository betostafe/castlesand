procedure OnTick;
begin
  if States.GameTime = 50 then
    Actions.ShowMsg(0, States.Text(0));
  if States.GameTime = 50 then
    Actions.ShowMsg(6, States.Text(0));
  if States.GameTime = 1800 then
    Actions.ShowMsg(0, States.Text(1));
  if States.GameTime = 1800 then
    Actions.ShowMsg(6, States.Text(1));
end;